{
  "name": "retext-diacritics",
  "version": "3.0.0",
  "description": "retext plugin to check for proper use of diacritics",
  "license": "MIT",
  "keywords": [
    "unified",
    "retext",
    "retext-plugin",
    "plugin",
    "diacritic",
    "loan",
    "words"
  ],
  "repository": "retextjs/retext-diacritics",
  "bugs": "https://github.com/retextjs/retext-diacritics/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Sylvan Swierkosz <sswierkosz@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "files": [
    "index.js",
    "schema.js"
  ],
  "dependencies": {
    "match-casing": "^2.0.0",
    "nlcst-search": "^3.0.0",
    "nlcst-to-string": "^3.0.0",
    "quotation": "^2.0.0",
    "unist-util-position": "^4.0.0"
  },
  "devDependencies": {
    "c8": "^7.0.0",
    "mdast-zone": "^5.0.0",
    "prettier": "^2.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "retext": "^8.0.0",
    "tape": "^5.0.0",
    "unist-builder": "^3.0.0",
    "xo": "^0.39.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --conditions development test.js",
    "test-coverage": "c8 --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 --reporter lcov node --conditions development test.js",
    "test": "npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      [
        "lint-table-pipe-alignment",
        false
      ],
      [
        "gfm",
        {
          "tablePipeAlign": false
        }
      ],
      "./script/table.js"
    ]
  }
}
